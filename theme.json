{
	"$schema": "https://schemas.wp.org/wp/6.8/theme.json",
	"version": 3,
	"settings": {
		"appearanceTools": true,
		"useRootPaddingAwareAlignments": true,
		"color": {
			"defaultDuotone": false,
			"defaultGradients": false,
			"defaultPalette": false,
			"duotone": [],
			"custom": true,
			"customDuotone": true,
			"customGradient": true,
			"link": true,
			"palette": [
				{
					"slug": "primary",
					"color": "#359EFF",
					"name": "Primary Blue"
				},
				{
					"slug": "primary-dark",
					"color": "#2E4F64",
					"name": "Primary Dark"
				},
				{
					"slug": "secondary",
					"color": "#BDE0FE",
					"name": "Secondary Light Blue"
				},
				{
					"slug": "green-blue",
					"color": "#006D77",
					"name": "Green Blue"
				},
				{
					"slug": "cta-yellow",
					"color": "#FFC300",
					"name": "CTA Yellow"
				},
				{
					"slug": "accent",
					"color": "#FFD28E",
					"name": "Accent Peach"
				},
				{
					"slug": "accent-dark",
					"color": "#1d2c33",
					"name": "Accent Dark"
				},
				{
					"slug": "accent-text",
					"color": "#332A1C",
					"name": "Accent Text"
				},
				{
					"slug": "background-light",
					"color": "#f5f7f8",
					"name": "Background Light"
				},
				{
					"slug": "background-dark",
					"color": "#0f1923",
					"name": "Background Dark"
				},
				{
					"slug": "text-light",
					"color": "#4A4A4A",
					"name": "Text Light"
				},
				{
					"slug": "text-dark",
					"color": "#E0E0E0",
					"name": "Text Dark"
				},
				{
					"slug": "footer-dark",
					"color": "#1C2541",
					"name": "Footer Dark"
				}
			]
		},
		"layout": {
			"contentSize": "1200px",
			"wideSize": "1440px"
		},
		"spacing": {
			"defaultSpacingSizes": false,
			"units": ["%", "px", "em", "rem", "vh", "vw"],
			"spacingSizes": [
				{
					"slug": "10",
					"size": "0.25rem",
					"name": "XX-Small"
				},
				{
					"slug": "20",
					"size": "0.5rem",
					"name": "X-Small"
				},
				{
					"slug": "30",
					"size": "0.75rem",
					"name": "Small"
				},
				{
					"slug": "40",
					"size": "1rem",
					"name": "Base"
				},
				{
					"slug": "50",
					"size": "clamp(1rem, 2vw, 1.5rem)",
					"name": "Medium"
				},
				{
					"slug": "60",
					"size": "clamp(1.5rem, 3vw, 2rem)",
					"name": "Large"
				},
				{
					"slug": "70",
					"size": "clamp(2rem, 4vw, 3rem)",
					"name": "X-Large"
				},
				{
					"slug": "80",
					"size": "clamp(3rem, 5vw, 4rem)",
					"name": "XX-Large"
				}
			]
		},
		"typography": {
			"defaultFontSizes": false,
			"fluid": true,
			"fontFamilies": [
				{
					"fontFamily": "Lexend, sans-serif",
					"name": "Lexend",
					"slug": "lexend",
					"fontFace": [
						{
							"fontFamily": "Lexend",
							"fontStyle": "normal",
							"fontWeight": "400 900",
							"fontDisplay": "swap",
							"src": ["file:./assets/fonts/lexend-variable.woff2"]
						}
					]
				}
			],
			"fontSizes": [
				{
					"slug": "xs",
					"size": "0.875rem",
					"name": "Extra Small",
					"fluid": false
				},
				{
					"slug": "sm",
					"size": "1rem",
					"name": "Small",
					"fluid": false
				},
				{
					"slug": "base",
					"size": "1.125rem",
					"name": "Base",
					"fluid": {
						"min": "1rem",
						"max": "1.125rem"
					}
				},
				{
					"slug": "md",
					"size": "1.25rem",
					"name": "Medium",
					"fluid": {
						"min": "1.125rem",
						"max": "1.25rem"
					}
				},
				{
					"slug": "lg",
					"size": "1.5rem",
					"name": "Large",
					"fluid": {
						"min": "1.25rem",
						"max": "1.5rem"
					}
				},
				{
					"slug": "xl",
					"size": "2rem",
					"name": "Extra Large",
					"fluid": {
						"min": "1.5rem",
						"max": "2rem"
					}
				},
				{
					"slug": "2xl",
					"size": "2.5rem",
					"name": "2X Large",
					"fluid": {
						"min": "2rem",
						"max": "2.5rem"
					}
				},
				{
					"slug": "3xl",
					"size": "3rem",
					"name": "3X Large",
					"fluid": {
						"min": "2.5rem",
						"max": "3rem"
					}
				}
			]
		},
		"border": {
			"radius": true,
			"color": true,
			"style": true,
			"width": true
		},
		"custom": {
			"borderRadius": {
				"default": "0.125rem",
				"lg": "0.25rem",
				"xl": "0.5rem",
				"full": "0.75rem"
			}
		}
	},
	"styles": {
		"color": {
			"background": "var(--wp--preset--color--background-light)",
			"text": "var(--wp--preset--color--text-light)"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--lexend)",
			"fontSize": "var(--wp--preset--font-size--base)",
			"fontWeight": "400",
			"lineHeight": "1.6"
		},
		"spacing": {
			"blockGap": "var(--wp--preset--spacing--50)",
			"padding": {
				"top": "0",
				"right": "var(--wp--preset--spacing--50)",
				"bottom": "0",
				"left": "var(--wp--preset--spacing--50)"
			}
		},
		"elements": {
			"button": {
				"color": {
					"background": "var(--wp--preset--color--cta-yellow)",
					"text": "var(--wp--preset--color--accent-text)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--base)",
					"fontWeight": "600"
				},
				"spacing": {
					"padding": {
						"top": "0.75rem",
						"right": "1.5rem",
						"bottom": "0.75rem",
						"left": "1.5rem"
					}
				},
				"border": {
					"radius": "var(--wp--custom--border-radius--full)"
				},
				"css": ".wp-element-button:hover, .wp-block-button__link:hover { background-color: #E6B000; color: var(--wp--preset--color--accent-text); } .wp-element-button:focus, .wp-block-button__link:focus { outline: 2px solid var(--wp--preset--color--primary); outline-offset: 2px; }"
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				},
				"typography": {
					"textDecoration": "none"
				},
				"css": "a:hover { color: var(--wp--preset--color--primary-dark); text-decoration: underline; } a:focus { outline: 2px solid var(--wp--preset--color--primary); outline-offset: 2px; }"
			},
			"heading": {
				"color": {
					"text": "var(--wp--preset--color--primary-dark)"
				},
				"typography": {
					"fontWeight": "700",
					"lineHeight": "1.2"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--3xl)",
					"fontWeight": "900",
					"lineHeight": "1.1"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--2xl)",
					"fontWeight": "800",
					"lineHeight": "1.15"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xl)",
					"fontWeight": "700",
					"lineHeight": "1.2"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--lg)",
					"fontWeight": "700",
					"lineHeight": "1.3"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--md)",
					"fontWeight": "600",
					"lineHeight": "1.4"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--base)",
					"fontWeight": "600",
					"lineHeight": "1.4"
				}
			}
		},
		"blocks": {
			"core/heading": {
				"elements": {
					"h1": {
						"typography": {
							"fontSize": "var(--wp--preset--font-size--3xl)",
							"fontWeight": "900",
							"lineHeight": "1.1"
						}
					},
					"h2": {
						"typography": {
							"fontSize": "var(--wp--preset--font-size--2xl)",
							"fontWeight": "800",
							"lineHeight": "1.15"
						}
					},
					"h3": {
						"typography": {
							"fontSize": "var(--wp--preset--font-size--xl)",
							"fontWeight": "700",
							"lineHeight": "1.2"
						}
					},
					"h4": {
						"typography": {
							"fontSize": "var(--wp--preset--font-size--lg)",
							"fontWeight": "700",
							"lineHeight": "1.3"
						}
					},
					"h5": {
						"typography": {
							"fontSize": "var(--wp--preset--font-size--md)",
							"fontWeight": "600",
							"lineHeight": "1.4"
						}
					},
					"h6": {
						"typography": {
							"fontSize": "var(--wp--preset--font-size--base)",
							"fontWeight": "600",
							"lineHeight": "1.4"
						}
					}
				}
			},
			"core/paragraph": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--base)",
					"lineHeight": "1.6"
				},
				"color": {
					"text": "var(--wp--preset--color--text-light)"
				},
				"spacing": {
					"margin": {
						"bottom": "var(--wp--preset--spacing--40)"
					}
				}
			},
			"core/image": {
				"border": {
					"radius": "var(--wp--custom--border-radius--xl)"
				},
				"spacing": {
					"margin": {
						"top": "var(--wp--preset--spacing--40)",
						"bottom": "var(--wp--preset--spacing--40)"
					}
				}
			},
			"core/list": {
				"spacing": {
					"padding": {
						"left": "var(--wp--preset--spacing--40)"
					},
					"margin": {
						"bottom": "var(--wp--preset--spacing--40)"
					}
				},
				"typography": {
					"lineHeight": "1.8"
				},
				"css": ".wp-block-list li { margin-bottom: var(--wp--preset--spacing--20); } .wp-block-list li::marker { color: var(--wp--preset--color--primary); }"
			},
			"core/button": {
				"variations": {
					"outline": {
						"border": {
							"color": "var(--wp--preset--color--primary)",
							"width": "2px",
							"style": "solid"
						},
						"color": {
							"text": "var(--wp--preset--color--primary)",
							"background": "transparent"
						},
						"css": ".wp-block-button__link.is-style-outline:hover { color: var(--wp--preset--color--background-light); background-color: var(--wp--preset--color--primary); }"
					},
					"secondary": {
						"color": {
							"background": "var(--wp--preset--color--primary)",
							"text": "var(--wp--preset--color--background-light)"
						},
						"css": ".wp-block-button__link.is-style-secondary:hover { background-color: var(--wp--preset--color--primary-dark); }"
					},
					"large": {
						"spacing": {
							"padding": {
								"top": "1rem",
								"right": "2rem",
								"bottom": "1rem",
								"left": "2rem"
							}
						},
						"typography": {
							"fontSize": "var(--wp--preset--font-size--lg)"
						}
					}
				}
			},
			"core/navigation": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--base)",
					"fontWeight": "500"
				},
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--accent-text)"
						}
					}
				},
				"css": ".wp-block-navigation a:hover { color: var(--wp--preset--color--primary); }"
			},
			"core/post-template": {
				"css": "display: grid; grid-template-columns: repeat(auto-fill, minmax(300px, 1fr)); gap: 2rem;"
			},
			"core/query-pagination": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--base)",
					"fontWeight": "600"
				},
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--primary)"
						}
					}
				}
			},
			"core/post-title": {
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--primary)"
						}
					}
				},
				"css": ".wp-block-post-title a:hover { color: var(--wp--preset--color--primary); text-decoration: none; }"
			},
			"core/columns": {
				"spacing": {
					"blockGap": "var(--wp--preset--spacing--50)"
				},
				"css": "@media (max-width: 781px) { .wp-block-columns { flex-direction: column; } .wp-block-column { flex-basis: 100% !important; } }"
			}
		}
	},
	"templateParts": [
		{
			"name": "header",
			"title": "Header",
			"area": "header"
		},
		{
			"name": "footer",
			"title": "Footer",
			"area": "footer"
		},
		{
			"name": "sidebar",
			"title": "Sidebar",
			"area": "uncategorized"
		}
	]
}
